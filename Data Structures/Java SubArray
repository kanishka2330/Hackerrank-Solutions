import java.io.*;
import java.util.*;

public class Solution {
    
    

    public static void main(String[] args) {
        /* Enter your code here. Read input from STDIN. Print output to STDOUT. Your class should be named Solution. */
        
        Scanner input = new Scanner(System.in);
        int n = input.nextInt();
        int[] arr = new int[n];
              
        for(int i = 0; i<arr.length; i++){
            arr[i] = input.nextInt();
            
        }
        
        int count = countNegSubArr(arr);
        System.out.println(count);
    }
        
        public static int countNegSubArr(int[] arr){
            int counter = 0;
            
        for(int j = 0; j<arr.length; j++){
            
            for(int k = j; k<=arr.length; k++){
                
        int[] subArr = Arrays.copyOfRange(arr, j, k);
        
            int sum = subArrSum(subArr);
              if(sum<0){
                  counter++;
              }
          }
        }
        return counter;
        }
        
        public static int subArrSum(int[] array){
            int sum = 0;
            for(int i =0; i<array.length; i++){
                sum = sum + array[i];
            }
            return sum;
        }
    
} 
